    "https://ssl.gstatic.com/",
    "https://www.googleapis.com/",
    "https://accounts.google.com/"
  ],
  "oauth2": {
    "client_id": "140224327941-54e8c7refmj3697retgf3c6ed8lcj1dp.apps.googleusercontent.com",
